<h3>新規商品登録</h3>
<%= form_with model: @item, url: 'admin/items/new', method: :get do |f| %>

 <h4>商品名</h4>
  <%= f.text_field :name %>
  
 <h4>商品説明</h4>
 <%= f.text_field :introduction %>
 
 <h4>ジャンル</h4>
 <%= f.text_field :genre_id %>
 
 <h4>税抜価格</h4>
 <%= f.text_field :price %>
  
   <%= f.submit '新規登録' %>
   
 <% end %>